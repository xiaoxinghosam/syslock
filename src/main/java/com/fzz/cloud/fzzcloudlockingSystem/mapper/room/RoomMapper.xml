<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fzz.cloud.fzzcloudlockingSystem.mapper.room.RoomMapper" >
 	<update id="updateRoomByLockId" parameterType="java.lang.String">
 		update room
 		set lock_id = #{lockId} where id = #{roomId}
 	</update>
</mapper>